@{
    ViewData["Title"] = "Numbers Page";
}

@model int[]
<div class="text-center">
    <h1 class="display-4">Here are some numbers</h1>
    <div class="border border-dark w-25 mx-auto p-2">
        @{
            foreach (int number in Model)
            {
                <p>@number</p>
            }
        }
    </div>
</div>